{
  "name": "discord-ascii-bot",
  "version": "2.0.0",
  "description": "A discord bot that creates ASCII art",
  "private": true,
  "main": "src/index.mjs",
  "scripts": {
    "start": "node .",
    "format": "prettier . --write",
    "test": "echo \"Error: no test specified\" && exit 1",
    "discord:register-commands": "node src/scripts/register-commands.mjs --target guild",
    "discord:register-commands:global": "node src/scripts/register-commands.mjs --target global"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darcros/discord-ascii-bot.git"
  },
  "keywords": [
    "ASCII",
    "Discord",
    "bot"
  ],
  "author": "Dario Crosa (darcros)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darcros/discord-ascii-bot/issues"
  },
  "homepage": "https://github.com/darcros/discord-ascii-bot#readme",
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/rest": "^0.3.0",
    "commander": "^9.0.0",
    "discord-api-types": "^0.26.1",
    "discord.js": "^13.6.0",
    "dotenv": "^14.3.2",
    "enquirer": "^2.3.6",
    "figlet": "^1.5.2",
    "got": "^12.0.1",
    "sharp": "^0.29.3"
  },
  "devDependencies": {
    "prettier": "^2.5.1"
  }
}
